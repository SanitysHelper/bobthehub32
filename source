	local spinbutton = createbutton("Misc", "Spin [Number], [Off]")
	local spinning = false
	spinbutton.MouseButton1Click:Connect(function()
		if lp.Character.HumanoidRootPart:FindFirstChild("HarambeSpin") then
			spinning = true
			spinbutton.Text = "Spin [Number], [Off]"
		else
			spinning = false
			spinbutton.Text = "Spin [Number], [On]"
		end
		if spinning == false then
			local spin = Instance.new("BodyAngularVelocity",lp.Character.HumanoidRootPart)
			spin.Name = "HarambeSpin"
			spin.MaxTorque = Vector3.new(0,'inf',0)
			spin.AngularVelocity = Vector3.new(0,gettext(),0)
		else
			repeat
				lp.Character.HumanoidRootPart:FindFirstChild("HarambeSpin"):Destroy()
			until not lp.Character.HumanoidRootPart:FindFirstChild("HarambeSpin")
		end	
	end) --7
	
	local noclipbutton = createbutton("Misc", "Noclip [Off]")
