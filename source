	createbutton("Ninja Simulator", "Crash", function()
		local function fix()
			spawn(function()
				game:GetService("ReplicatedStorage").Remotes.FixChar:FireServer();
			end)
		end
		for i = 1,20 do
			spawn(function()
				while game:GetService"RunService".Stepped:Wait() do
					for i = 1,50 do
						fix()
					end
				end
			end)
		end
	end)
	
	createbutton("Ninja Simulator", "Increase sword damage", function()
		for i,v in pairs(lp.Backpack:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v:FindFirstChildWhichIsA("LocalScript"):Clone().Parent = v
			end
		end
	
		for i,v in pairs(lp.Character:GetChildren()) do
			if v:FindFirstChild"Katana" then
				v:FindFirstChildWhichIsA("LocalScript"):Clone().Parent = v
			end
		end
	end)
	---------------------------------------------------------------------------------------------------------------------------- Mortem Metallum
	createframe("Mortem Metallum")
	
	createbutton("Mortem Metallum", "Auto Parry [Lasts until you rejoin]", function()
		function getnearestplayer()
			local dist = math.huge
			local chosen = nil
			for i,v in pairs(game.Players:GetPlayers()) do
				if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild"Head" then
					local mag = v:DistanceFromCharacter(game.Players.LocalPlayer.Character.Head.Position)
					if mag < dist and v ~= game.Players.LocalPlayer then
						dist = mag
						chosen = v
					end
				end
			end
			return chosen, dist
		end
	
		function parry() 
			equipweapon() 
			game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").ability:FireServer()
			repeat wait() until game.Players.LocalPlayer.Character:FindFirstChild"Parry"
			repeat wait() until not game.Players.LocalPlayer.Character:FindFirstChild"Parry"
			equipweapon()
		end
	
		function weaponcheck(part)
			if part and part ~= nil and part ~= "" then
				local name = part.Name
				if name ~= "Snow ball" and name ~= "Explosive present" and name ~= "Grenade" and name ~= "C4" and name ~= "Fire bomb" and name ~= "Bear trap" and name ~= "Firework" and name ~= "Festive Cannon" and name ~= "Trumpet" and name ~= "Health potion" then
					return true
				end
			end
		end
	
		function equipweapon()
			local player = game.Players.LocalPlayer
			for i,v in pairs(player.Character:GetChildren()) do
				if v:IsA("Tool") and not weaponcheck(v) then
					v.Parent = player.Backpack
				end
			end
			for i,v in pairs(player.Backpack:GetChildren()) do
				if weaponcheck(v) and v:IsA("Tool") then
					v.Parent = player.Character
					break
				end
			end
		end
	
		function getplayersweapon(player)
			player = game.Players[player.Name]
			if player.Character:FindFirstChildWhichIsA"Tool" and weaponcheck(player.Character:FindFirstChildWhichIsA"Tool") then
				return player.Character:FindFirstChildWhichIsA"Tool".wep.Value
			end
		end
	
		function getattacking(player)
			local weapon = getplayersweapon(player)
			if weapon then
				return weapon.Iron.slash.Playing
			end
		end
	
		function getnearestattacking()
			local plr, dist = getnearestplayer()
			if math.abs(dist) <= 12.5 then
				if getattacking(plr) then
					return plr
				end
			end
		end
	
		game:GetService("RunService").Stepped:connect(function()
			local attacking = getnearestattacking()
			if attacking then
				parry()
			end
		end)
	end)
	
	createbutton("Mortem Metallum", "Xbox Controls", function()
		local function weaponcheck(part)
			if part and part ~= nil and part ~= "" then
				local name = part.Name
				if name ~= "Snow ball" and name ~= "Explosive present" and name ~= "Grenade" and name ~= "C4" and name ~= "Fire bomb" and name ~= "Bear trap" and name ~= "Firework" and name ~= "Festive Cannon" and name ~= "Trumpet" and name ~= "Health potion" then
					return true
				end
			end
		end
	
		local function equipweapon(tool)
			if not tool then
				local player = game.Players.LocalPlayer
				for i,v in pairs(player.Character:GetChildren()) do
					if v:IsA("Tool") and not weaponcheck(v) then
						v.Parent = player.Backpack
					end
				end
				for i,v in pairs(player.Backpack:GetChildren()) do
					if weaponcheck(v) and v:IsA("Tool") then
						v.Parent = player.Character
						break
					end
				end
			else
				local tool = lp.Backpack:FindFirstChild(tool)
				if tool then
					for i,v in pairs(lp.Character:GetChildren()) do
						if v:IsA("Tool") then
							v.Parent = lp.Backpack
						end
					end
					wait()
					tool.Parent = lp.Character
				end
			end
		end
	
		local function parry()
			equipweapon()
			game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Tool").ability:FireServer()
		end
	
		local function roll()
			local hrp = lp.Character:WaitForChild("HumanoidRootPart")
			local humanoid = lp.Character:WaitForChild("Humanoid")
			local pos = hrp.Position
			local velocity = hrp.Velocity
			local direction = humanoid.MoveDirection
			local A_1 = direction
			local A_2 = pos
			local A_3 = velocity
			local Event = game:GetService("ReplicatedStorage").dash
			if direction ~= Vector3.new(0,0,0) then
				Event:FireServer(A_1, A_2, A_3)
			end
		end
	
		game:GetService("UserInputService").InputBegan:Connect(function(key)
			if key.KeyCode == Enum.KeyCode.ButtonX then
				roll()
			end
	
			if key.KeyCode == Enum.KeyCode.ButtonL2 then
				parry()
			end
	
			if key.KeyCode == Enum.KeyCode.ButtonL3 then
				lp.Character.Runscript.run:FireServer(true)
				repeat wait() until key.UserInputState == Enum.UserInputState.End
				lp.Character.Runscript.run:FireServer(false)
			end
	
			if key.KeyCode == Enum.KeyCode.DPadUp then
				equipweapon()
			end
	
			if key.KeyCode == Enum.KeyCode.DPadDown then
				equipweapon("Fire bomb")
			end
	
			if key.KeyCode == Enum.KeyCode.DPadRight then
				equipweapon("Grenade")
			end
	
			if key.KeyCode == Enum.KeyCode.DPadLeft then
				equipweapon("C4")
			end    
	
			if key.KeyCode == Enum.KeyCode.ButtonY then
				local A_1 = 29
				local Event = game:GetService("ReplicatedStorage").Item
				Event:FireServer(A_1)
			end
	
			if key.KeyCode == Enum.KeyCode.ButtonB then
				local A_1 = game:GetService("Workspace").PlayersCharacters.ErasedGuestUser
				local Event = game:GetService("ReplicatedStorage").kicc
				Event:FireServer(A_1)
			end
		end)
	
	end)
	
	--371112664------------------------------Faction Defence----------------------------------------------
	
	createframe("Faction Defence")
	
	local factionpressednoob = false
	local factionfarm = createbutton("Faction Defence", "Auto farm")
	factionfarm.MouseButton1Click:Connect(function()
		if factionpressednoob == false then
			factionpressednoob = true
			factionfarm.Text = "Used"
			function givemoney(amount)
				local A_1 = math.random(1,5)
				local Event = game:GetService("ReplicatedStorage").PlayerMoneyUpdate[game.Players.LocalPlayer.Name]
				Event:FireServer(A_1)
			end
	
			function getitem(item)
				local A_1 = item
				local Event = game:GetService("ReplicatedStorage").Events.Purchase
				Event:InvokeServer(A_1)
			end
			game:GetService("CoreGui").PurchasePromptApp.PurchasePromptUI.Visible = false
	
			_G.FDT = not _G.FDT
			while _G.FDT == true do
				wait(5)
				for i,v in pairs(workspace.Camera:GetChildren()) do
					if v:IsA("Model") and v:FindFirstChild("Cost") then
						spawn(function()
							givemoney(tonumber(v.Cost.Value))
							getitem(v.Name)
						end)
					end
				end
			end
		end
	end)
	
	createbutton("Faction Defence", "Give money [amount]", function()
		local A_1 = gettext()
		local Event = game:GetService("ReplicatedStorage").PlayerMoneyUpdate[game.Players.LocalPlayer.Name]
		Event:FireServer(A_1)
	end)
	
	local godmodefaction = createbutton("Faction Defence", "God char [Off]")
	
	godmodefaction.MouseButton1Click:Connect(function()
		_G.GODMANOWU = not _G.GODMANOWU
		if _G.GODMANOWU == true then
			godmodefaction.Text = "God char [On]"
		else
			godmodefaction.Text = "God char [Off]"
		end
		if _G.GODMANOWU == true then
			game.Players.LocalPlayer.Character:WaitForChild("Humanoid"):Destroy()
		end
		local godmanxd = game.Players.LocalPlayer.CharacterAdded:Connect(function(char)
			local human = char:WaitForChild("Humanoid")
			local sadlifexd = human.HealthChanged:Connect(function()
				local A_1 = "Damage"
				local A_2 = -334543456356456345364345653465534
				local A_3 = human
				local Event = game:GetService("ReplicatedStorage").Swords
				Event:InvokeServer(A_1, A_2, A_3)
			end)
			repeat wait() until _G.GODMANOWU == false
			sadlifexd:Disconnect()
		end)
	
		repeat wait() until _G.GODMANOWU == false
		godmanxd:Disconnect()
	end)
	
	createbutton("Faction Defence", "More troops", function()
		local A_1 = "Buy a Soldier - $100"
		local Event = game:GetService("ReplicatedStorage").Events.Purchase
		Event:InvokeServer(A_1)
	end)
	
	local goddiefaction = createbutton("Faction Defence", "God mode [Off]")
	goddiefaction.MouseButton1Click:Connect(function()
		_G.GodingModeFactionDnut = not _G.GodingModeFactionDnut
		if _G.GodingModeFactionDnut == true then
			goddiefaction.Text = "God mode [On]"
		else
			goddiefaction.Text = "God mode [Off]"
		end
	
		function checkai(v)
			if v:IsA("Humanoid") and not v:IsDescendantOf(game.Players.LocalPlayer.Character) and v.Parent:FindFirstChild("crater") then
				if tostring(v.Parent.crater.Value):match(game.Players.LocalPlayer.Name) or tostring(v.Parent.crater.Value):match("HamburberHelper") then
					v.HealthChanged:Connect(function()
						spawn(function()
							pcall(function()
								local A_1 = "Damage"
								local A_2 = -334543456356456345364345653465534
								local A_3 = v
								local Event = game:GetService("ReplicatedStorage").Swords
								Event:InvokeServer(A_1, A_2, A_3)
							end)
						end)
					end)
					spawn(function()
						pcall(function()
							local A_1 = "Damage"
							local A_2 = -334543456356456345364345653465534
							local A_3 = v
							local Event = game:GetService("ReplicatedStorage").Swords
							Event:InvokeServer(A_1, A_2, A_3)
						end)
					end)
				end
			elseif v:IsA("Humanoid") and v:IsDescendantOf(game.Players.LocalPlayer.Character) then
				local donutitoeat = v.HealthChanged:Connect(function()
					spawn(function()
						pcall(function()
							local A_1 = "Damage"
							local A_2 = -33456345364345653465534
							local A_3 = v
							local Event = game:GetService("ReplicatedStorage").Swords
							Event:InvokeServer(A_1, A_2, A_3)
						end)
					end)
				end)
				spawn(function()
					repeat wait() until _G.GodingModeFactionDnut == false
					donutitoeat:Disconnect()
				end)
				spawn(function()
					pcall(function()
						local A_1 = "Damage"
						local A_2 = -334543456356364345653465534
						local A_3 = v
						local Event = game:GetService("ReplicatedStorage").Swords
						Event:InvokeServer(A_1, A_2, A_3)
					end)
				end)
			elseif v:IsA("Humanoid") and game.Players:FindFirstChild"HamburberHelper" and v:IsDescendantOf(game.Players:FindFirstChild"HamburberHelper".Character) then
				local pooeinoob = v.HealthChanged:Connect(function()
					spawn(function()
						pcall(function()
							local A_1 = "Damage"
							local A_2 = -33456345364345653465534
							local A_3 = v
							local Event = game:GetService("ReplicatedStorage").Swords
							Event:InvokeServer(A_1, A_2, A_3)
						end)
					end)
				end)
	
				spawn(function()
					repeat wait() until _G.GodingModeFactionDnut == false
					pooeinoob:Disconnect()
				end)
	
				spawn(function()
					pcall(function()
						local A_1 = "Damage"
						local A_2 = -334543456356364345653465534
						local A_3 = v
						local Event = game:GetService("ReplicatedStorage").Swords
						Event:InvokeServer(A_1, A_2, A_3)
					end)
				end)
			end
		end
	
	
		for i,v in pairs(workspace:GetDescendants()) do
			if _G.GodingModeFactionDnut == true then
				checkai(v)
			end
		end
		local workihard = workspace.DescendantAdded:Connect(function(thing)
			if _G.GodingModeFactionDnut == true then
				wait(1)
				checkai(thing)
			end
		end)
		repeat wait() until _G.GodingModeFactionDnut == false
		workihard:Disconnect()
	end)
	
	createbutton("Faction Defence", "Kill all", function()
		function damage(v)
			spawn(function()
				local A_1 = "Damage"
				local A_2 = math.huge
				local A_3 = v
				local Event = game:GetService("ReplicatedStorage").Swords
				Event:InvokeServer(A_1, A_2, A_3)
				Event:InvokeServer("Tag", 0, A_3)
			end)
		end
	
		for i,v in pairs(workspace:GetDescendants()) do
			if v:IsA("Humanoid") then
				damage(v)
			end
		end
	end)
	
	createframe("Ragdoll Engine")
	
	local push = createbutton("Ragdoll Engine", "Push [Off]")
	local pushing = false
	push.MouseButton1Click:Connect(function()
		pushing = not pushing
		if pushing == true then
			push.Text = "Super Push [On]"
		else
			push.Text = "Super Push [Off]"
		end
		if pushing == true then
			while pushing == true and game:GetService("RunService").Stepped:Wait() do
				local Event = game.Players.LocalPlayer.Character:FindFirstChild("Push") or game.Players.LocalPlayer.Backpack:FindFirstChild("Push")
				if Event then
					Event = Event.PushEvent
					Event:FireServer()
				end
			end
		end
	end)
	
	antirag = createbutton("Ragdoll Engine", "Anti ragdoll [Off]")
	local norag = false
	antirag.MouseButton1Click:Connect(function()
		norag = not norag
		if norag == true then
			antirag.Text = "Anti ragdoll [On]"
		else
			antirag.Text = "Anti ragdoll [Off]"
		end
		if norag == true then
			local harderpushman = game.Players.LocalPlayer.Character.HumanoidRootPart.ChildAdded:Connect(function(v)
				if v:IsA("BodyForce") then
					game:GetService("RunService").Stepped:Wait()
					v:Destroy()
				end
			end)
			while game:GetService"RunService".Stepped:Wait() and norag == true do
				local bool_1 = false;
				local Target = game:GetService("ReplicatedStorage").LocalRagdollEvent;
				Target:Fire(bool_1);
			end
			harderpushman:Disconnect()
		end
	end)
	
	------------------------End--------------------------------------------
	
	local loaded = Instance.new("BoolValue", mainframe.Parent)
	loaded.Name = "Loaded"
	loaded.Value = true
	mainframe.Parent.Text = name
	
	charadded(function()
		spinbutton.Text = "Spin [Number], [Off]"
		flybutton.Text = "Fly [Off]"
		unfly()
		unswim()
		sitbutton.Text = "Sit [Off]"
		platformedgy.Text = "Platform stand [Off]"
		gethumanoid().Changed:Connect(function()
			sitting = gethumanoid().Sit
			if sitting == true then
				sitbutton.Text = "Sit [On]"
			else
				sitbutton.Text = "Sit [Off]"
			end
		end)
	end)
end
coroutine.wrap(EKLPFOS_fake_script)()
local function FKTVMTF_fake_script() -- Minimize.LocalScript 
	local script = Instance.new('LocalScript', Minimize)

	local timeer = 0.5
	local mainframe = script.Parent.Parent.MainFrame
	local title = mainframe.Parent
	local pressed = false
	mainframe:TweenSize(UDim2.fromScale(mainframe.Size.X.Scale,0), Enum.EasingDirection.In,Enum.EasingStyle.Linear,0,false,function()
		title.Values.Visible = false
		mainframe.Visible = false
		title.BackgroundTransparency = 0.7
		title:TweenSize(UDim2.fromScale(0.255/2,title.Size.Y.Scale),Enum.EasingDirection.In,Enum.EasingStyle.Linear,0,false,function()
			pressed = true
		end)
	end)
	script.Parent.MouseButton1Click:Connect(function()
		if pressed == false then
			mainframe:TweenSize(UDim2.fromScale(mainframe.Size.X.Scale,0), Enum.EasingDirection.In,Enum.EasingStyle.Linear,timeer,false,function()
				title.Values.Visible = false
				mainframe.Visible = false
				title:TweenSize(UDim2.fromScale(0.255/2,title.Size.Y.Scale),Enum.EasingDirection.In,Enum.EasingStyle.Linear,timeer,false,function()
					title.BackgroundTransparency = 0.7
					pressed = true
				end)
			end)
		else
			mainframe.Visible = true
			title.BackgroundTransparency = 0
			mainframe:TweenSize(UDim2.fromScale(mainframe.Size.X.Scale,10), Enum.EasingDirection.In,Enum.EasingStyle.Linear,timeer,false,function()
				title.Values.Visible = true
				title:TweenSize(UDim2.fromScale(0.255,title.Size.Y.Scale),Enum.EasingDirection.In,Enum.EasingStyle.Linear,timeer,false,function()
					pressed = false
				end)
			end)
		end
	end)
	
	repeat wait() until mainframe.Parent:FindFirstChild("Loaded")
	mainframe.Visible = true
	title.BackgroundTransparency = 0
	mainframe:TweenSize(UDim2.fromScale(mainframe.Size.X.Scale,10), Enum.EasingDirection.In,Enum.EasingStyle.Linear,timeer,false,function()
		title.Values.Visible = true
		title:TweenSize(UDim2.fromScale(0.255,title.Size.Y.Scale),Enum.EasingDirection.In,Enum.EasingStyle.Linear,timeer,false,function()
			pressed = false
		end)
	end)
end
coroutine.wrap(FKTVMTF_fake_script)()
