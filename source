	createbutton("Misc", "Kill [Player]", function()
		local plr = game.Players.LocalPlayer
	
		function godmode()
			local char = game.Players.LocalPlayer.Character
			local humanoid = char:WaitForChild("Humanoid")
			local humanoid2 = humanoid:Clone()
			char.Parent = nil
			humanoid.Parent = nil
			char.Parent = workspace
			char.Parent = nil
			humanoid2:Destroy()
			humanoid.Parent = char
			char.Parent = workspace
		end
	
		function equiptools()
			local tools = {}
			for i,v in pairs(plr.Backpack:GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					v.Parent = plr.Character
					tools[#tools+1] = v
				end
			end
			for i,v in pairs(plr.Character:GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChild("Handle") then
					v.Parent = plr.Character
					tools[#tools+1] = v
				end
			end
			return tools, #tools
		end
	
		function to(player)
			local finished = false
			repeat
				local tools = equiptools()
				for i,v in pairs(tools) do
					v.Handle.Position = player.Character.HumanoidRootPart.Position
				end
				if #tools == 0 then
					finished = true
				end
				wait()
			until finished == true
		end
	
		function suicide()
			plr.Character.Humanoid.Health = 0
			plr.Character = nil
		end
	
		function kill(player)
			if game.Players:FindFirstChild(player) and plr.Backpack:FindFirstChildWhichIsA("Tool") or plr.Character and plr.Character:FindFirstChildWhichIsA("Tool") then
				local victim = game.Players[player]
				godmode()
				to(victim)
				wait()
				suicide()
				local currenttime = tick()
				repeat wait() until not victim or victim.Character.Humanoid.Health <= 0 or not victim.Character or tick() > currenttime + 2
				if not victim then
					print(""..player.." left")
				elseif victim.Character.Humanoid.Health <= 0 then
					print(""..player.." died")
				else
					print("respawned or didn't kill")
				end
			elseif not game.Players:FindFirstChild(player) then
				print("player doesn't exist idiot")
			elseif not plr.Character then
				print("thanoed")
			else
				print("you need a tool dumbass")
			end
		end
	
		kill(getplr(gettext()).Name)
	end) --15
	
	createbutton("Misc", "Bring [Player]", function()
		local textio = gettext()
		local oldcframe = getroot().CFrame
		local function attach()
			local humanoid = gethumanoid()
			humanoid.Parent.Animate.Disabled = true
			local humanoid2 = humanoid:Clone()
			humanoid2.Parent = humanoid.Parent
			humanoid:Destroy()
			humanoid2.Parent.Animate.Disabled = false
			game.Workspace.Camera.CameraSubject = game.Players.LocalPlayer.Character
			humanoid2.DisplayDistanceType = Enum.HumanoidDisplayDistanceType.None
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
				if v:IsA("Tool") and v:FindFirstChildWhichIsA("BasePart") then
					v.Parent = lp.Character
				end
			end
		end
		local victim = getplr(textio)
		local victimroot = getroot(victim)
		local oldpos = getroot().CFrame
		attach()
		local hrp = getroot() 
		local done = false
		spawn(function()
			lp.CharacterAdded:Wait():WaitForChild("HumanoidRootPart").CFrame = oldpos
		end)
		repeat
			game:GetService("RunService").Stepped:Wait()
			if hrp and hrp.Parent ~= nil and hrp.Parent.Parent ~= nil and gettools("char") <= 0 then
				hrp.CFrame = oldcframe
			elseif hrp and hrp.Parent ~= nil and hrp.Parent.Parent ~= nil  then
				getroot(victim).CFrame = getroot().CFrame + getroot().CFrame.LookVector.Unit * 1.5
			else
				done = true
			end
		until lp.Character == nil or not getroot() or done == true or not getplr(textio) or getplr(textio).Character == nil
	
	end) --16
	
	createbutton("Misc", "Rejoin", function()
		game:GetService("TeleportService"):Teleport(game.PlaceId)
	end) --17
	
	createbutton("Misc", "Health Bars for All", function()
		function healthbar(plr)
			local HealthBar = Instance.new("BillboardGui")
			local TextLabel = Instance.new("TextLabel")
			local ImageLabel = Instance.new("ImageLabel")
			local HealthBar_2 = Instance.new("Frame")
			local Bar = Instance.new("Frame")
			local UIGradient = Instance.new("UIGradient")
	
			HealthBar.Name = "Health Bar"
			HealthBar.Parent = plr.Character:WaitForChild"Head"
			HealthBar.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
			HealthBar.Active = true
			HealthBar.ExtentsOffsetWorldSpace = Vector3.new(0, 3, 0)
			HealthBar.LightInfluence = 0.500
			HealthBar.Size = UDim2.new(0, 150, 0, 100)
			HealthBar.SizeOffset = Vector2.new(0, 0.25)
	
			TextLabel.Parent = HealthBar
			TextLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel.BackgroundTransparency = 1.000
			TextLabel.Position = UDim2.new(0, 0, 0.5, 0)
			TextLabel.Size = UDim2.new(1, 0, 0.129999995, 0)
			TextLabel.Font = Enum.Font.Highway
			TextLabel.Text = plr.Name
			TextLabel.TextColor3 = Color3.fromRGB(0, 0, 0)
			TextLabel.TextScaled = true
			TextLabel.TextSize = 14.000
			TextLabel.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel.TextStrokeTransparency = 0.500
			TextLabel.TextWrapped = true
	
			ImageLabel.Parent = HealthBar
			ImageLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ImageLabel.BackgroundTransparency = 1.000
			ImageLabel.Position = UDim2.new(0.333333343, 0, 0, 0)
			ImageLabel.Size = UDim2.new(0.333333343, 0, 0.5, 0)
			ImageLabel.Image = "http://www.roblox.com/asset/?id=4868892267"
			ImageLabel.ImageColor3 = Color3.fromRGB(255, 0, 0)
			ImageLabel.Visible = false
	
			HealthBar_2.Name = "HealthBar"
			HealthBar_2.Parent = HealthBar
			HealthBar_2.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			HealthBar_2.BorderColor3 = Color3.fromRGB(91, 91, 91)
			HealthBar_2.BorderSizePixel = 3
			HealthBar_2.Position = UDim2.new(0.0299999996, 0, 0.75, 0)
			HealthBar_2.Size = UDim2.new(0.959999979, 0, 0.0700000003, 0)
	
			local humanoid = plr.Character:WaitForChild("Humanoid")
			humanoid.NameDisplayDistance = 9
			humanoid.HealthDisplayDistance = 0
			humanoid.HealthChanged:Connect(function(health)
				local maxhealth = humanoid.MaxHealth
				local percent = health/maxhealth
				Bar.Size = UDim2.fromScale(percent, 1)
			end)
	
			Bar.Name = "Bar"
			Bar.Parent = HealthBar_2
			Bar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Bar.Size = UDim2.new(1, 0, 1, 0)
	
			UIGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(129, 129, 129)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(255, 255, 255))}
			UIGradient.Rotation = 270
			UIGradient.Parent = Bar
		end
	
		function charadded(plr, scriptio)
			spawn(function()
				scriptio()
			end)
			plr.CharacterAdded:Connect(function()
				spawn(function()
					scriptio()
				end)
			end)
		end
	
		repeat wait() until game:IsLoaded()
	
		for i,v in pairs(game.Players:GetPlayers()) do
			if v.Name ~= game.Players.LocalPlayer.Name then
				charadded(v, function()
					healthbar(v)
				end)
			end
		end
	
		game.Players.PlayerAdded:Connect(function(v)
			charadded(v, function()
				healthbar(v)
			end)
		end)
	end) --18
	
	local charm = lp.Character
	
	function unswim()
		workspace.Gravity = 198.5
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Climbing,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.FallingDown,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Flying,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Freefall,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.GettingUp,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Jumping,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Landed,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Physics,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.PlatformStanding,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Ragdoll,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Running,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Seated,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics,true)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Swimming,true)
		gethumanoid():ChangeState(Enum.HumanoidStateType.RunningNoPhysics)
	end
	
	function swim()
		unswim()
		workspace.Gravity = 0
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Climbing,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.FallingDown,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Flying,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Freefall,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.GettingUp,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Jumping,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Landed,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Physics,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.PlatformStanding,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Ragdoll,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Running,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.RunningNoPhysics,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Seated,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.StrafingNoPhysics,false)
		gethumanoid():SetStateEnabled(Enum.HumanoidStateType.Swimming,false)
		gethumanoid():ChangeState(Enum.HumanoidStateType.Swimming)
	end
	
	local swimbutton = createbutton("Misc", "Swim [Off]")
	local swimming = false
	swimbutton.MouseButton1Click:Connect(function()
		swimming = not swimming
		if swimming == true then
			swim()
			swimbutton.Text = "Swim [On]"
		else
			unswim()
			swimbutton.Text = "Swim [Off]"
		end
	end) --19
	
	local platformedgy = createbutton("Misc", "Platform stand")
	platformedgy.MouseButton1Click:Connect(function()
		local textpoo = function()
			if not gethumanoid().PlatformStand == false then
				return "Off"
			else
				return "On"
			end
		end
		platformedgy.Text = "Platform stand [".. textpoo() .."]" 
		gethumanoid().PlatformStand = not gethumanoid().PlatformStand
	end)
	
	createbutton("Misc", "Enable shift lock", function()
		lp.DevEnableMouseLock = true                         
	end)
	
